#!/usr/bin/env bash
#
# Run dmenu with bar configuration and colors

source environment-colors
source ~/.config/bar/config

dmenu_run -fn "$font" \
          -w "$width" \
          -h "$height" \
          -x "$offset_x" \
          -y "$offset_y" \
          -nf "$c_fg" \
          -nb "$c_bg" \
          -sf "$c_fg" \
          -sb "$c_hl"
